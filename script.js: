// Поки що просто заготовка для карти
function initMap() {
    const mapOptions = {
        center: { lat: 50.4501, lng: 30.5236 }, // Київ
        zoom: 12,
    };
    const map = new google.maps.Map(document.getElementById("map"), mapOptions);
}

function loadMap() {
    const script = document.createElement('script');
    script.src = "https://maps.googleapis.com/maps/api/js?key=ВАШ_КЛЮЧ&callback=initMap";
    script.async = true;
    script.defer = true;
    document.body.appendChild(script);
}

window.onload = loadMap;
